api:
  dashboard: true
  debug: true
entryPoints:
  http:
    address: ":80"
  https:
    address: ":443"
serversTransport:
  insecureSkipVerify: true
providers:
  docker:
    endpoint: "unix:///var/run/docker.sock"
    exposedByDefault: false
  file:
    filename: /config.yml
log:
  filePath: "/logs/traefik.log"
  #level: DEBUG
certificatesResolvers:
  myresolver:
    acme:
      email: {{letsencrypt_email}}
      storage: acme.json
      #CA Staging server.  comment out to go to prod server
      #caServer: "https://acme-staging-v02.api.letsencrypt.org/directory"
      dnsChallenge:
        provider: porkbun
        resolvers:
          - "curitiba.porkbun.com"
          - "fortaleza.porkbun.com"
          - "maceio.porkbun.com"
          - "salvador.porkbun.com"
